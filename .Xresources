Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull

URxvt.termName: xterm-256color
URxvt*scrollBar: false

URxvt.font: xft: Ubuntu Mono:style=Regular:size=11, \
            -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1, \
            xft: Font Awesome 5 Free:style=Regular, \
            xft: Font Awesome 5 Free:style=Solid, \
            xft: Font Awesome 5 Brands:style=Regular
URxvt*selectToClipboard: true
URxvt*letterSpace: 0

URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800

! special
*.foreground:   #ffffff
*.background:   #000000
*.cursorColor:  #ffffff

! foreground color for underline
URxvt*colorUL:            #8ae234
! line color for underline
URxvt*underlineColor:     #92659a
! black dark/light
URxvt*color0:             #2e3436
URxvt*color8:             #6e706b
! red dark/light
URxvt*color1:             #cc0000
URxvt*color9:             #ef2929
! green dark/light
URxvt*color2:             #4e9a06
URxvt*color10:            #8ae234
! yellow dark/light
URxvt*color3:             #edd400
URxvt*color11:            #fce94f
! blue dark/light
URxvt*color4:             #3465a4
URxvt*color12:            #729fcf
! magenta dark/light
URxvt*color5:             #92659a
URxvt*color13:            #c19fbe
! cyan dark/light
URxvt*color6:             #07c7ca
URxvt*color14:            #63e9e9
! white dark/light
URxvt*color7:             #d3d7cf
URxvt*color15: #eeeeec
